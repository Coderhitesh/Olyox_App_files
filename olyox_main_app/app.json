{
  "expo": {
    "name": "Olyox",
    "slug": "Olyox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/playstore-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash_screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#da2e2a"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher-web.png",
        "backgroundColor": "#da2e2a"
      }
    },
    "web": {
      "favicon": "./assets/ic_launcher-web.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
