{
  "expo": {
    "name": "Olyox",
    "slug": "olyox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/playstore-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
     "jsEngine": "jsc",
    "splash": {
      "image": "./assets/playstore-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.olyoxuser.olyox",  
      "development": {
        "distribution": "internal",
        "buildType": "simulator"
      },
      "production": {
        "distribution": "app-store"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app requires your location to provide better services.",
        "CFBundleURLTypes": [],
        "ITSAppUsesNonExemptEncryption": false,
        "GMSApiKey": "AIzaSyBvyzqhO8Tq3SvpKLjW7I5RonYAtfOVIn8"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher-web.png",
        "backgroundColor": "#ffffff"
      },
      "googleMaps": {
        "apiKey": "AIzaSyBvyzqhO8Tq3SvpKLjW7I5RonYAtfOVIn8"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "host": "auth/callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.happy_coding.olyox"
    },
    "web": {
      "favicon": "./assets/ic_launcher-web.png",
      "build": {
        "publicPath": "/"
      }
    },
    "extra": {
      "clerkFrontendApi": "pk_test_c2VjdXJlLWdudS0zNi5jbGVyay5hY2NvdW50cy5kZXYk",
      "clerkApiKey": "pk_test_c2VjdXJlLWdudS0zNi5jbGVyay5hY2NvdW50cy5kZXYk",
      "eas": {
        "projectId": "e73c718b-d6aa-45ea-a12a-c496424892ba"
      }
    }
  }
}
